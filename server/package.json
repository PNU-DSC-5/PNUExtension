{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "start": "set PORT=3000 && ts-node ./bin/www.ts",
    "start:mac": "PORT=3000 ts-node ./bin/www.ts",
    "start:debug": "set PORT=3000 && nodemon --watch *.ts --exec ts-node ./bin/www.ts",
    "start:debug:mac": "PORT=3000 && nodemon --watch ./ --exec ts-node ./bin/www.ts --inspect=9229",
    "prebuild": "rimraf dist",
    "build": "webpack --config webpack.config.js",
    "postbuild": "cp -r ./public ./dist/public",
    "start:production": "NODE_ENV=production node ./dist/bundle.js",
    "start:production:window": "set NODE_ENV=production && node ./dist/bundle.js",
    "test": "jest --coverage --detectOpenHandles --forceExit --passWithNoTests"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/http-errors": "^1.8.0",
    "@types/mysql": "^2.15.15",
    "@types/passport": "^1.0.4",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-github": "^1.1.5",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-kakao": "^0.2.0",
    "@types/passport-local": "^1.0.33",
    "@types/passport-naver": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.1.1",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "jsonfile": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-naver": "^1.0.6",
    "react-google-places-autocomplete": "^3.0.0",
    "tslib": "^2.0.2",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.48",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^5.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "main": "index.js",
  "license": "MIT"
}
